export default undefined;

// AUFGABE ðŸ¤”:
// Verarbeite die RÃ¼ckgabe von Typ unknown so, dass du damit vernÃ¼nftig arbeiten kannst

type Something = {
  type: string,
  amount: number
}

function getSomething(what: string): unknown {
  if (what === 'beer') {
    return {
      type: 'bootles',
      amount: 10
    };
  } else {
    return 'n/a';
  }
}

// const what = 'ale';
const what = 'beer';
const something = getSomething('beer');
